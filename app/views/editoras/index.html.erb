<p id="notice"><%= notice %></p>

<h1>Editoras</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @editoras.each do |editora| %>
      <tr>
        <td><%= editora.nome %></td>
        <td><%= link_to 'Show', editora %></td>
        <td><%= link_to 'Edit', edit_editora_path(editora) %></td>
        <td><%= link_to 'Destroy', editora, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Editora', new_editora_path %>
